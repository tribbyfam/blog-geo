<div class="row">
  <div class="col s12 m7">
    <div class="card">
      <div class="card-image">
        <img src="images/sample-1.jpg">
        <span class="card-title">Profile</span>
      </div>
      <div class="card-content">
        <%= user.id.pics %>
      </div>
      <div>
        <%= user.id.first_name %>
      </div>
      <div>
        <%= user.id.last_name %>
      </div>
      <div>
        <%= user.id.birthday %>
        <div>
        <%= user.id.username %>
      </div>
      <div>
        <%= user.id.email %>
      </div>
    </div>
  </div>
</div>